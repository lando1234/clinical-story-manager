{
  "name": "clinical-story-manager",
  "version": "0.1.0",
  "private": true,
  "overrides": {
    "path-to-regexp": "^6.3.0",
    "undici": "^6.11.1",
    "esbuild": "^0.25.0"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall-prisma.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate && node scripts/create-prisma-index.js",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage",
    "test:db:create": "bash scripts/create-test-db.sh",
    "test:db:setup": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:db:reset": "dotenv -e .env.test -- npx prisma migrate reset",
    "pre-commit:build": "npm run db:generate && npm run build",
    "pre-commit:test": "npm test",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "next": "16.0.10",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "vercel": "^50.1.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.0.1",
    "vitest": "^3.2.3"
  }
}
